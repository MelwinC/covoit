{
  "extends": "@adonisjs/tsconfig/tsconfig.app.json",
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./build",
    "baseUrl": ".",
    "types": ["@types/jest"],
    "exclude": ["node_modules", "build"],
    "paths": {
      "#models/*": ["app/models/*"],
      "#controllers/*": ["app/controllers/Http/*"],
      "#middleware/*": ["app/middleware/*"],
      "#utils/*": ["app/utils/*"]
    }
  }
}
